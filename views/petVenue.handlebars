    <!-- This is the main tag to keep the footer sticky! -->
    <main>
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-12">
                    <h1 id="mainHeader">Your Places:</h1>
                </div>
            </div>
            <div class="row">
                {{#each places}}
                <div class="col-sm-4">
         
                        <img src="/images/dog_{{this.type}}.png" class="img-fluid" alt="Pet Friendly Places" >

                    <button id={{this.id}} type="button" class="placeModal btn btn-primary btn-lg btn-block" data-toggle="modal" data-target=".bd-example-modal-lg">{{this.name}} <i class="fas fa-paw"></i></button>
                </div>
                {{/each}}
            </div>
        </div>
    </main>

