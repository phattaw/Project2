<!-- This is the modal to post hotel! -->

    <div class="modal" tabindex="-1" role="dialog" id="postHotelModal">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Post a Hotel!</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form class="reviewForm">
                        <div class="form-group">
                            <label for="nameHotel">Name of Hotel:</label>
                            <input type="text" class="form-control" name="name" id="nameHotel" placeholder="Hotel Name">
                        </div>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="inputCity">City:</label>
                                <input type="text" class="form-control" name="city" id="inputCity" placeholder="City">
                            </div>
                            <div class="form-group col-md-6">
                                <label for="inputState">State:</label>
                                <input type="text" class="form-control" name="state" id="inputState" placeholder="State">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="reviewHotel">Review of Hotel:</label>
                           <textarea class="form-control" id="reviewHotel" rows="3" name="review" placeholder="Hotel Review"></textarea>
                        </div>
                        <div class="form-group">
                            <label for="linkHotel">Link to Hotel's Website:</label>
                            <input type="text" class="form-control" id="linkHotel" name="website" placeholder="Hotel Website">
                        </div>
                        <div class="form-group">
                            <label for="inputPhoto">Photo:</label>
                            <input type="file" class="form-control-file" id="inputPhoto">
                        </div>
                        <input type="hidden" name="type" value="hotel">
                        <button type="submit" class="btn btn-dark btn-lg">Post Hotel <i class="fas fa-paper-plane"></i></button>
                    </form>
                </div>
            </div>
        </div>
    </div>

